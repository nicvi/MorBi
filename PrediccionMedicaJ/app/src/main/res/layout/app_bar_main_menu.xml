<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OperationOverPatient.menuCrearPaciente">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/Theme.PrediccionMedicaJ.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="#b0f9f7"
            android:theme="@style/ToolbarColoredBackArrow"
            app:popupTheme="@style/Theme.PrediccionMedicaJ.PopupOverlay"
            app:subtitleTextColor="#373737"
            app:titleTextColor="#373737" />

    </com.google.android.material.appbar.AppBarLayout>

    <include layout="@layout/content_main_menu" />

    <!--
    <com.google.android.material.floatingactionbutton.FloatingActionButton
